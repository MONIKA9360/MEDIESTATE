generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id         String   @id @default(uuid())
  name       String
  email      String
  phone      String
  message    String
  propertyId String?
  createdAt  DateTime @default(now())
}

model LoanLead {
  id        String   @id @default(uuid())
  name      String
  email     String
  phone     String
  subject   String
  message   String
  createdAt DateTime @default(now())
}
